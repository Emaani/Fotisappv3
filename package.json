{
  "name": "fotisapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next && next clean",
    "test:db": "ts-node scripts/test-db.ts",
    "test:email": "cross-env NODE_ENV=production ts-node --require dotenv/config scripts/test-email.ts dotenv_config_path=.env.email",
    "postbuild": "next-sitemap",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "prisma db seed",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "postinstall": "prisma generate",
    "db:init": "cross-env NODE_ENV=production ts-node --transpile-only scripts/init-db.ts",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "verify:deployment": "cross-env NODE_ENV=production ts-node scripts/verify-deployment.ts",
    "deploy:prod": "npm run pre-deploy && vercel --prod",
    "verify:email": "cross-env NODE_ENV=production ts-node scripts/verify-email-config.ts",
    "pre-deploy": "cross-env NODE_ENV=production ts-node scripts/pre-deploy-check.ts",
    "generate:secret": "ts-node scripts/generate-auth-secret.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@chakra-ui/react": "^3.1.1",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.3.0",
    "@sentry/nextjs": "^8.51.0",
    "@types/passport": "^1.0.16",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-google-oauth20": "^2.0.16",
    "@vercel/analytics": "^1.4.1",
    "@vercel/postgres": "^0.10.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "boxicons": "^2.1.4",
    "chart.js": "^4.4.7",
    "chartjs-adapter-date-fns": "^3.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.21.0",
    "framer-motion": "^10.16.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.441.0",
    "next": "^14.2.20",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "sequelize": "^6.37.4",
    "ts-node": "^10.9.2",
    "validator": "^13.12.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.9",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-router-dom": "^5.3.3",
    "@types/sequelize": "^4.28.20",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.10",
    "postcss": "^8",
    "prisma": "^6.3.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.6.2"
  }
}
